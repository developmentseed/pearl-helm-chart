---
layout: none
---
{%- assign lulc-helm = site.data.index.entries.lulc-helm | sort: 'created' | reverse -%}

{%- assign lulc_helm_latest = lulc-helm | first -%}

{%- capture "lulc_helm_stable_version" -%}
{%- for chart in lulc-helm -%}
    {%- unless chart.version contains "-" -%}
        {{ chart.version }};
    {%- endunless -%}
{%- endfor -%}
{%- endcapture -%}
{%- assign lulc_helm_stable_version = lulc_helm_stable_version | split: ";" | first -%}

{%- capture "lulc_helm_pre_version" -%}
{%- for chart in lulc-helm -%}
    {%- assign dots_count_after_dash = chart.version | split: "-" | shift | first | split: "." | size | minus: 1 -%}
    {%- assign first_char_after_dash = chart.version | split: "-" | shift | first | slice: 0 -%}
    {%- if chart.version contains "-" -%}
        {%- if dots_count_after_dash == 1 -%}
            {%- if first_char_after_dash != "n" -%}
                {{ chart.version }};
            {%- endif -%}
        {%- endif -%}
    {%- else -%}
        {{ chart.version }};
    {%- endif -%}
{%- endfor -%}
{%- endcapture -%}
{%- assign lulc_helm_pre_version = lulc_helm_pre_version | split: ";" | first -%}


{
    "lulc-helm":
        {
            "stable": "{{ lulc_helm_stable_version }}",
            "pre": "{{ lulc_helm_pre_version }}",
            "dev": "{{ lulc_helm_latest.version }}",
            "latest": "{{ lulc_helm_latest.version }}"
        }
}
